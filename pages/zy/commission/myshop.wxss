page {
    background: #F2f2f2;
}

.container {
    background-color: #F2f2f2;
    min-height: 100%;
}

.userinfo {
    width: 100%;
    display: flex;
    margin-top: 24rpx;
    padding-top: 72rpx;
    padding-bottom: 24rpx;
    flex-direction: row;
    background: #fff;
}

.useritems {
    flex: 1;
    height: 76rpx;
    padding: 12rpx 0;
    border-right: 1rpx solid #eaeaea;
    font-size: 28rpx;
}

.useritems:first-of-type {
    display: flex;
    justify-content: center;
    align-items: flex-end;
}

.useritem {
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
    align-items: center;
}

.useritems:last-of-type {
    border-right: 0rpx;
}

.useritems .avatar {
    width: 134rpx;
    height: 134rpx;
}

.useritems .text {
    text-align: center;
    line-height: 36rpx;
}

.useritems .icon {
    display: flex;
    justify-content: center;
    text-align: center;
    margin-bottom: 48rpx;
}

.useritems .icon image {
    width: 48rpx;
    height: 48rpx;
}

.header {
    width: 95%;
    margin: 0 auto;
    margin-top: 10px;
    flex-direction: row;
}

.header .icon {
    width: 40rpx;
    height: 40rpx;
    vertical-align: top;
    margin-left: -15rpx;
    margin-top: 0.45rem;
}

.header .search {
    width: auto;
    color: #fff;
    font-size: 0.85rem;
    line-height: 2rem;
    padding-left: 30rpx;
    border-radius: 0.3rem;
    background: #E64340;
    margin-bottom: 9px;
    display: block;
}

.type-navbar {
    white-space: nowrap;
    display: flex;
    height: 188rpx;
    background-color: #fff;
}

.type-boxs {
    width: 100%;
    box-sizing: content-box;
    font-size: 24rpx;
    line-height: 84rpx;
    text-align: center;
    overflow: hidden;
    font-family: 微软雅黑;
}

.type-navbar-items {
    font-size: 0.85rem;
    display: flex;
    flex-direction: row;
}

.type-item-on {
    border-bottom: 4rpx solid #e64340;
}

.goods-container {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    box-sizing: content-box;
    padding: 24rpx;
}

.goods-box {
    width: 339rpx;
    height: 472rpx;
    background-color: #fff;
    overflow: hidden;
    margin-bottom: 24rpx;
}

.goods-box .img-box {
    width: 339rpx;
    height: 339rpx;
    overflow: hidden;
}

.goods-box .img-box image {
    width: 339rpx;
    height: 339rpx;
}

.goods-box .goods-title {
    width: 280rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 0.85rem;
    padding: 24rpx 0 0rpx 0;
    color: #000;
    margin-left: 24rpx;
}

.goods-box .goods-price {
    width: 280rpx;
    overflow: hidden;
    font-size: 0.85rem;
    padding: 24rpx 0;
    color: #e64340;
    margin-left: 24rpx;
}

.no-more-goods {
    text-align: center;
    font-size: 24rpx;
    padding-bottom: 48rpx;
    color: #999;
}

.type-container {
    width: 100%;
    height: 150rpx;
    background-color: #fff;
    overflow: hidden;
    padding-bottom: 10px;
}

.slide-image-navs {
    height: 2.4rem;
    width: 2.4rem;
    margin-top: 12%;
}

.type-container_shop {
    width: 100%;
    min-height: 84rpx;
    background-color: #fff;
    overflow: hidden;
    border-bottom: 1rpx solid #eaeaea;
}

.type-boxs {
    width: 100%;
    box-sizing: content-box;
    font-size: 24rpx;
    line-height: 84rpx;
    min-height: 84rpx;
    text-align: center;
    font-family: 微软雅黑;
}

.type-navbar-items {
    width: 100%;
    line-height: 84rpx;
    min-height: 84rpx;
    font-size: 0.85rem;
    display: flex;
    flex-direction: row;
}

.first_nav {
    margin-top: -22%;
    width: 100%;
    height: 9px;
    float: left;
    font-size: 0.85rem;
    color: #666;
}

.shop_index_style {
    flex: 1;
    line-height: 82rpx;
    height: 82rpx;
    text-align: center;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    color: #999;
    letter-spacing: 5px;
}