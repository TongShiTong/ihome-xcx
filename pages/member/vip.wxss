.fui-list {
    padding: 36rpx 24rpx;
}

.fui-list-inner {
    display: flex;
    height: 200rpx;
    flex-direction: column;
    justify-content: space-between;
}

.fui-list-inner .des {
    font-size: 26rpx;
    line-height: 36rpx;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    /* white-space: pre-wrap; */
}

.fui-list-inner .person {
    min-width: 90rpx;
    text-align: center;
