/* pages/area/designer.wxss */
.bj {
  width: 100%;
  height: 177rpx;
  background: #6B0415;
}
.anli-tabs {
  width: 710rpx;
  height: 275rpx;
  border-radius: 16rpx;
  background: #fff;
  box-shadow:0px 2rpx 8rpx 0px #ccc;
  display: flex;
  justify-content: space-between;
  padding: 30rpx 20rpx 20rpx;
  margin: 0 auto;
  box-sizing: border-box;
  position: relative;
  z-index: 1;
}
.tabs {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
}
.tabs image {
  width: 120rpx;
  height: 120rpx;
}
.label {
  font-size: 24rpx;
  color: #aaa;
  text-align: center;
}
.anli {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin: 34rpx 30rpx 0;
}
.anli-info {
  border-radius: 16rpx;
  width: 330rpx;
  background: #fff;
  overflow: hidden;
  padding-bottom: 30rpx;
  margin: 0 0 30rpx;
}
.image {
  width: 100%;
  min-height: 300rpx;
}
.image image {
  width: 100%;
  min-height: 300rpx;
}
.title {
  font-size: 28rpx;
  color: #333;
  margin: 10rpx 10rpx 20rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.xx-info {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 20rpx;
  font-size: 24rpx;
  color: #999;
}
.toux {
  width: 60rpx;
  height: 60rpx;
}
.toux image{
  width: 100%;
  height: 100%;
}
.yuyeu {
  width: 143rpx;
  height: 127rpx;
  /* border-radius: 50%; */
  position: fixed;
  top: 65%;
  right: 30rpx;
  /* background: #fff; */
  /* font-size: 28rpx;
  line-height: 120rpx;
  text-align: center; */
}
.yuyeu image {
  width: 100%;
  height: 100%
}
