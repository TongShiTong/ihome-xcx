.anli {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin: 34rpx 30rpx 0;
}
.anli-info {
  border-radius: 16rpx;
  width: 330rpx;
  background: #fff;
  overflow: hidden;
  padding-bottom: 30rpx;
  margin: 0 0 30rpx;
}
.image {
  width: 100%;
  min-height: 300rpx;
}
.image image {
  width: 100%;
  min-height: 300rpx;
}
.title {
  font-size: 28rpx;
  color: #333;
  margin: 10rpx 10rpx 20rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.xx-info {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 20rpx;
  font-size: 24rpx;
  color: #999;
}
.toux {
  width: 60rpx;
  height: 60rpx;
}
.toux image{
  width: 100%;
  height: 100%;
}
.fby-box {
  position: fixed;
  top: 50%;
  right: 30rpx;
}
