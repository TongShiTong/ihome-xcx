/* pages/area/designer.wxss */
.yuyue {
  width: 690rpx;
  height: 80rpx;
  margin: 0 auto;
  border-radius: 16rpx;
  background: #fff;
  text-align: center;
  line-height: 80rpx;
}
.anli {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin: 0 20rpx;
}
.anli-info {
  border-radius: 16rpx;
  width: 340rpx;
  background: #fff;
  overflow: hidden;
  padding-bottom: 30rpx;
}
.image {
  width: 100%;
  min-height: 200rpx;
}
.image image {
  width: 100%;
  min-height: 200rpx;
}
.title {
  font-size: 28rpx;
  color: #333;
  margin: 10rpx 10rpx 20rpx;
}
.xx-info {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 20rpx;
  font-size: 24rpx;
  color: #999;
}
.toux {
  width: 60rpx;
  height: 60rpx;
}
.toux image{
  width: 100%;
  height: 100%;
}
.yuyeu1 {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  position: fixed;
  top: 80%;
  right: 0;
  background: #fff;
  font-size: 28rpx;
  line-height: 120rpx;
  text-align: center;
}